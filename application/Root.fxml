<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Main">
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="500.0">
         <children>
            <TextField fx:id="usernameField" layoutX="176.0" layoutY="90.0" promptText="Username" />
            <PasswordField fx:id="passwordField" layoutX="176.0" layoutY="125.0" promptText="Password" />
            <Button layoutX="228.0" layoutY="165.0" mnemonicParsing="false" onAction="#login" text="Login" />
            <Button layoutX="201.0" layoutY="205.0" mnemonicParsing="false" onAction="#createAccount" text="Create Account" />
            <Button fx:id="exitButton" layoutX="233.0" layoutY="245.0" mnemonicParsing="false" onAction="#exitApplication" text="Exit" />
            <Label layoutX="155.0" layoutY="30.0" prefHeight="25.0" prefWidth="190.0" text="Contractor Marketplace" textAlignment="CENTER">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
