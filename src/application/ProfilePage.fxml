<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="application.ProfileController">
  <children>
    <AnchorPane prefHeight="300.0" prefWidth="500.0">
      <children>
        <Label fx:id="profileNameLabel" layoutX="14.0" layoutY="14.0" prefHeight="43.0" prefWidth="484.0" text="User Profile: (companyname)">
          <font>
            <Font size="18.0" />
          </font>
        </Label>
        <Label fx:id="companyTypeLabel" layoutX="14.0" layoutY="49.0" text="Company Type: (TYPE)">
          <font>
            <Font size="14.0" />
          </font>
        </Label>
        <Label fx:id="profileBox" alignment="TOP_LEFT" contentDisplay="CENTER" layoutX="25.0" prefHeight="150.0" prefWidth="450.0" text="write stuff" AnchorPane.topAnchor="95.0" />
        <Button fx:id="backButton" layoutX="445.0" layoutY="260.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
        <Button fx:id="editButton" layoutX="400.0" layoutY="260.0" mnemonicParsing="false" onAction="#editProfile" text="Edit" />
        <TextArea fx:id="profileEditBox" layoutX="25.0" layoutY="55.0" prefHeight="175.0" prefWidth="450.0" promptText="Add your information here, then press save" visible="false" />
        <Button fx:id="saveButton" layoutX="200.0" layoutY="260.0" mnemonicParsing="false" onAction="#saveEdit" text="Save" visible="false" />
        <Button fx:id="cancelEditButton" layoutX="260.0" layoutY="260.0" mnemonicParsing="false" onAction="#cancelEdit" text="Cancel" visible="false" />
      </children>
    </AnchorPane>
  </children>
</AnchorPane>
