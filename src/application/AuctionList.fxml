<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="300.0" prefWidth="500.0">
         <children>
            <Label layoutX="14.0" layoutY="9.0" prefHeight="41.0" prefWidth="372.0" text="Active Auctions">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TableView fx:id="auctionTable" layoutX="10.0" layoutY="50.0" prefHeight="203.0" prefWidth="480.0">
              <columns>
                <TableColumn fx:id="selectionColumn" editable="false" prefWidth="25.0" />
                <TableColumn fx:id="titleColumn" editable="false" prefWidth="139.0" text="Title" />
                  <TableColumn fx:id="locColumn" editable="false" prefWidth="92.0" text="Location" />
                  <TableColumn fx:id="creatorColumn" editable="false" prefWidth="73.0" text="Posted By" />
                  <TableColumn fx:id="currentColumn" editable="false" prefWidth="70.0" text="Current Bid" />
                  <TableColumn fx:id="dateColumn" editable="false" text="Date Posted" />
              </columns>
            </TableView>
            <Button layoutX="434.0" layoutY="261.0" mnemonicParsing="false" text="Back" />
            <Button layoutX="320.0" layoutY="261.0" mnemonicParsing="false" text="Advanced Search" />
            <Button layoutX="250.0" layoutY="261.0" mnemonicParsing="false" text="View/Bid" />
         </children></AnchorPane>
   </children>
</AnchorPane>
